{
  "expo": {
    "name": "harukinsite",
    "slug": "harukinsite",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "scheme": "haruk.in"
  }
}
